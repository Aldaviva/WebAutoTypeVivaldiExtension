{
	"manifest_version": 2,
	"name": "WebAutoType",
	"version": "0.0.0",
	"author": "Ben Hutchison",
	"description": "Send the active URL from Vivaldi to the KeePass WebAutoType plugin so it can autotype the correct password.",
	"icons": {
		"16": "icons/KeePass_Round_16.png",
		"32": "icons/KeePass_Round_32.png",
		"48": "icons/KeePass_Round_48.png",
		"128": "icons/KeePass_Round_128.png"
	},
	"background": {
		"page": "background.html",
		"persistent": false
	},
	"permissions": [
		"tabs",
		"storage",
		"http://127.0.0.1:53372/webautotype/activeurl/vivaldi"
	],
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"externally_connectable": {
		"ids": [ "mpognobbkildjkofajifpdfhcoklimli" ]
	},
	"key": ""
}